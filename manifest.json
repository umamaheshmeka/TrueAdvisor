{
    "name": "True Adviser",
    "version": "1.0",
    "description": "Scrap any website",
    "permissions": ["downloads","activeTab","declarativeContent","storage"],
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' https://unpkg.com/@google/model-viewer/dist/model-viewer.js; object-src 'self'",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "192": "images/man.png"

          }
    },
    "icons": {
        "192": "images/man.png"

      },
    "manifest_version": 2
  }